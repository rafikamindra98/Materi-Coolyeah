unit Unit1;

interface

uses
  Windows, Messages, SysUtils, Variants, Classes, Graphics, Controls, Forms,
  Dialogs, StdCtrls;

type
  TForm1 = class(TForm)
    Label1: TLabel;
    Label2: TLabel;
    Label3: TLabel;
    Edit1: TEdit;
    Edit2: TEdit;
    Button1: TButton;
    Edit3: TEdit;
    procedure Button1Click(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  Form1: TForm1;

implementation

{$R *.dfm}

procedure TForm1.Button1Click(Sender: TObject);
  var NamaDepan,NamaBelakang,NamaLengkap:string;
begin
   NamaDepan:=Edit1.Text;
   NamaBelakang:=Edit2.Text;
   NamaLengkap:=NamaDepan+' '+NamaBelakang;
   Edit3.Text:=NamaLengkap;
end;

end.
