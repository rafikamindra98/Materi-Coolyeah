	int n, a, b, c, i;
   	cout<<"N = ";cin>>n;
   	b = 1;
   	c = 1;
   	for (i=3; i<=n; i++) {
   		cout<<b;
   		cout<<c;
   		a=b+c;
   		cout<<a;
   		b=c;
   		a=b;
	   } 
