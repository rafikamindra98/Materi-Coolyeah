b. 	int i,j,n;
   	cout<<"Masukkan bilangan: ";cin>>n;
   	for (i=1; i<=n; i++) {
	   for (j=1; j<=i; j++) {
	   cout<<"*";
	   }
	   cout<<endl;
	   } 
d.	int main() {
	int i,j,n;
   	cout<<"Masukkan bilangan: ";cin>>n;
   	for (i=1; i<=n; i++) {
	   for (j=n; j>=i; j--) {
	   cout<<"*";}
	   cout<<endl;
	   } 
e. 	int i,j,n;
   	cout<<"Masukkan bilangan: ";cin>>n;
   	for (i=1; i<=n; i++) {
	   for (j=0; j<=n-i; j++) {
	   cout<<"*";
	   }
	   cout<<endl;
	   } 
