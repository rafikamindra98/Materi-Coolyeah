	const int NMax = 10;
	int A[NMax];
	int B[NMax];
	int i, j, k, n;
	cout<<"N = "; cin>>n;
	cout<<"1"<<endl;
	A[1]=1;
   	A[2]=2;
    	A[3]=1;
    	cout<<"1 2 1"<<endl;
    	for(i=3; i<=n; i++) {
    	B[1]=1;
    	for(j=2; j<=i; j++) {
    		B[j]=A[j-1]+A[j];
		}
		B[j+1] = 1;
		for(k=1; k<=i+1; k++) {
			cout<<B[k];
		}
		for (k=1; k<=i+1; k++) {
			A[k]=B[k];
		}
	}
