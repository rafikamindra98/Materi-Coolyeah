	int n, i;
	bool flag;
	cout<<"Masukkan bilangan: ";cin>>n;
	flag=false;
	for(i=2; i<=n-1; i++) {
		if((n%i)==0)
		flag=true;
	}
	if (flag=false) {
		cout<<n<<" adalah bilangan prima."<<endl;
	} else {
		cout<<n<<" bukan bilangan prima."<<endl;
	}
