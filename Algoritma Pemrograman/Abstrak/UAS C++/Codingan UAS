	int i, j, k, n;
   	cout<<"N = ";cin>>n;
   	for (k=1; k<=n; k++) {
   		for (j=1; j<=n; j++) {
   			for (i=1; i<=n; i++) {
   				if (i*i+j*j==k*k) {
   					cout<<"k = "<<k<<", j = "<<j<<", i = "<<i<<endl;
				   }
			   }
		   }
===================================================================
	int i, j, k, n;
   	cout<<"N = ";cin>>n;
   	for (k=1; k<=n; k++) {
   		for (j=1; j<=n; j++) {
   			for (i=1; i<=n; i++) {
   				if (k*k+j*j==i*i) {
   					cout<<"k = "<<k<<", j = "<<j<<", i = "<<i<<endl;
				   }
			   }
		   }
	   } 
